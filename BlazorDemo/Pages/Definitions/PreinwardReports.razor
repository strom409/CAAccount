   <div class="card p-3 mb-4" style="border: 1px solid lightgray; background-color: #f5f5f5;">
    <div class="row g-3 align-items-end">
        <!-- Date From -->
        <div class="col-sm-2">
            <label for="date1" class="form-label">Dated From</label>
            <DxDateEdit @bind-Date="EditModel.PreinwardDate"
                       CssClass="cw-320"
                       style="height: 30px;" InputId="date1" />
        </div>

        <!-- Date To -->
        <div class="col-sm-2">
            <label for="date2" class="form-label">Dated To</label>
            <DxDateEdit @bind-Date="EditModel.PreinwardDateTo"
                       CssClass="cw-320"
                       style="height: 30px;" InputId="date2" />
        </div>

        <!-- Grower Group Name -->
        <div class="col-md-2">
            <label for="growerGroup" class="form-label">Grower Group Name</label>
            <DxComboBox @key="GrowerGroupKey" 
                       Data="@GetgrowerList"
                       SearchMode="@SearchMode"
                       SearchFilterCondition="@SearchFilterCondition"
                       SearchTextParseMode="@SearchTextParseMode"
                       ValueChanged="@(async (string? newValue) => await OnGrowerGroupChanged(newValue))"
                       TextFieldName="GrowerName"
                       ValueFieldName="GrowerName"
                       CssClass="cw-480"
                       Value="@EditModel.GrowerGroupName"
                       InputId="growerGroup" 
                      NullText="Grower Group"
                       ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
        </div>

        <!-- Grower Name -->
        <div class="col-md-2">
            <label for="growerName" class="form-label">Grower Name</label>
            <DxComboBox @key="GrowerNameKey" 
                       Data="@GetSubGrowerGroup"
                       SearchMode="@SearchMode"
                       SearchFilterCondition="@SearchFilterCondition"
                       SearchTextParseMode="@SearchTextParseMode"
                       Value="@EditModel.GrowerName"
                       ValueChanged="@(async (string? newValue) => await OnSubgrowerChanged(newValue))"
                       TextFieldName="GrowerName"
                       ValueFieldName="GrowerName"
                       CssClass="cw-480"
                       InputId="growerName"
                         NullText="Grower Name"
                       ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
        </div>

        <!-- Product Name -->
        <div class="col-sm-2">
            <label for="productName" class="form-label">Product Name</label>
            <DxComboBox Data="@Growers"
                       SearchMode="@SearchMode"
                       SearchFilterCondition="@SearchFilterCondition"
                       SearchTextParseMode="@SearchTextParseMode"
                       Value="EditModel.Prodname"
                       TextFieldName="Itemname"
                       ValueFieldName="Itemname"
                       placeholder="No"
                       CssClass="custom-input-height"
                       InputId="productName"
                         NullText="Product Name"
                       ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
        </div>

        <!-- UOM -->
        <div class="col-sm-2">
            <label for="uom" class="form-label">UOM</label>
            <select id="uom" class="form-select form-select-sm w-100"
                   style="height: 30px;" @bind="EditModel.PreInwardUom" required>
                <option disabled selected value="">Select</option>
                <option value="Crate">Crate</option>
                <option value="Petti">Petti</option>
            </select>
        </div>

        <!-- Vehicle No -->
        <div class="col-md-2">
            <label for="vehicleNo" class="form-label">Vehicle No</label>
            <DxComboBox Data="@GetVehlist"
                       SearchMode="@SearchMode"
                       ListRenderMode="ListRenderMode.Virtual"
                       SearchFilterCondition="@SearchFilterCondition"
                       SearchTextParseMode="@SearchTextParseMode"
                       @bind-Value="EditModel.Vehno"
                       TextFieldName="Vehno"
                       ValueFieldName="Vehno"
                       CssClass="cw-480"
                       InputId="vehicleNo" 
NullText="Vehicle No"   
                       ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
        </div>

        <!-- Common Search -->
        <div class="col-md-2">
                    <label class="form-label">Common Search</label>
                    <input type="text" min="0" id="remarks"
                               style="font-size: 14px; height: 30px; font-family: 'Source Sans Pro', sans-serif;"
                               class="form-control input-placeholder-dim input-focus-dark"
                               @bind="EditModel.CommonSearch"
                               placeholder="Common search" />
                </div>
        <!-- Report Type -->
        <div class="col-sm-2">
            <label for="reportType" class="form-label">Report Type</label>
            <select id="reportType" class="form-select form-select-sm w-100"
                   style="height: 30px;" @bind="EditModel.PreInwardUom" required>
                <option disabled selected value="">Select</option>
                <option value="Grower">Grower</option>
                <option value="Date">Date</option>
                <option value="Summary">Summary</option>

            </select>
        </div>

        <div class="col-sm-2">
            <label for="reportType" class="form-label">Status</label>
            <select id="reportType" class="form-select form-select-sm w-100"
                    style="height: 30px;" @bind="EditModel.PreInwardStatus" required>
                <option disabled selected value="">Select</option>
                <option value="ALL">ALL</option>
                <option value="Completed">Completed</option>
                <option value="Pending">Pending</option>

            </select>
        </div>

        <!-- Action Buttons - Now properly aligned -->
        <div class="col-sm-2">
            <label class="form-label" style="visibility: hidden;">Actions</label>
            <div class="d-flex gap-2">
                <button @onclick="SearchCrates" 
                        class="btn btn-primary btn-sm flex-fill" 
                        style="height: 30px; white-space: nowrap;">
                    <i class="fa fa-search"></i> Search
                </button>
                <button @onclick="ClearFilter" 
                        class="btn btn-secondary btn-sm flex-fill" 
                        style="height: 30px; white-space: nowrap;">
                    Clear
                </button>
            </div>
        </div>
    </div>
</div>

  

                
