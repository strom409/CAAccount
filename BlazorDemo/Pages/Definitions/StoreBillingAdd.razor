<div class="card p-3 mb-4">
    <div class="row g-3 align-items-end">

        <div class="col-md-2">
            <label for="txtToDate">To Date<span style="color: red">*</span></label>
            <InputDate @bind-Value="@EditModel.Billdate" style="height: 30px;" class="form-control" id="txtToDate" />
        </div>

        <div class="col-md-2">
            <label for="crateType" class="form-label">Type</label>
            <select id="crateType" class="form-select form-select-sm w-100"
                    style="height: 33px;" @bind="EditModel.Billtype" required>
                <option disabled selected value="">Select</option>
                <option value="CA">CA</option>
                <option value="Lotwise">Lotwise</option>
            </select>
        </div>

        <div class="col-md-2">
            <label for="txtToDate">Grower Group<span style="color: red">*</span></label>
            <DxComboBox @key="GrowerGroupKey" Data="@GetgrowerList"
                        SearchMode="@SearchMode"
                        SearchFilterCondition="@SearchFilterCondition"
                        NullText="Select..."
                        SearchTextParseMode="@SearchTextParseMode"
                        ValueChanged="@(async (string? newValue) => await OnGrowerGroupChanged(newValue))"
                        TextFieldName="GrowerGroupName"
                       
                        ValueFieldName="GrowerGroupName"
                        CssClass="small-input"
                        Value="@EditModel.GrowerGroupName"
                        Enabled="@isComboDisabled"
                        InputId="cbFiltering"
                        ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
        </div>

        <!-- Chamber with inline button -->
        <div class="col-md-2">
            <div class="d-flex align-items-end">
                <div class="flex-grow-1">
                    <label for="txtToDate">Chamber<span style="color: red">*</span></label>
                    <DxComboBox Data="@GetChambers"
                                SearchMode="@SearchMode"
                                ValueChanged="@(async (string? newValue) => await OnChamberGroupChanged(newValue))"
                                Value="@EditModel.ChamberName"
                                SearchFilterCondition="@SearchFilterCondition"
                                SearchTextParseMode="@SearchTextParseMode"
                                TextFieldName="ChamberName"
                                ValueFieldName="ChamberName"
                                placeholder="No"
                                CssClass="small-input"
                                InputId="cbFiltering"
                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                </div>
                <button type="button" class="btn btn-sm btn-default ms-2 p-1"
                        style="min-width: 30px; height: 33px; color:black; background-color:lightgrey; margin-bottom: 0;"
                        @onclick="ViewDemands"
                        title="View Chamber">
                    <i class="fa fa-eye"></i>
                </button>
            </div>
     
</div>
        <div class="col-md-2">
            <label for="txtToDate">Bill To<span style="color: red">*</span></label>
            <DxComboBox @key="GrowerNameKey" Data="@GetSubGrowerGroup"
                        NullText="Select..."
                        SearchMode="@SearchMode"
                        SearchFilterCondition="@SearchFilterCondition"
                        SearchTextParseMode="@SearchTextParseMode"
                        Value="@EditModel.GrowerName"
                        ValueChanged="@(async (string? newValue) => await OnSubgrowerChanged(newValue))"
                        TextFieldName="GrowerName"
                        ValueFieldName="GrowerName"
                        DropDownVisibleChanged="OnSubDropDownChanged"
                        @onfocus="HandleFocusSub"
                        InputCssClass="small-input"
                        InputId="cbFiltering"
                        ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
        </div>

        <!-- Button Column - Now inline with the inputs -->
        <div class="col-sm-2 d-flex align-items-end">
            <button type="button" class="btn btn-sm btn-default me-2" style="min-width: 80px; color:white; background-color:#5A5E60" @onclick="SearchLots">
                <i class="fa fa-search"></i> Preview
            </button>

            <button type="button" class="btn btn-sm btn-default" style="min-width: 80px; color:black; background-color:lightgrey" @onclick="SearchLotsfinal">
                Upload
            </button>
        </div>



        @if (IsLoading)
        {
            <div class="text-center">
                <div class="spinner-border" role="status">
                    <span class="visually-hidden">Loading...</span>
                </div>
                <p>Loading demands...</p>
            </div>
        }

@if (GetBilldemand != null && GetBilldemand.Any())
{
    <div class="bill-demands-container">
        <!-- Select All Checkbox -->
        <div class="form-check mb-3">
            <input class="form-check-input" 
                   type="checkbox" 
                   id="selectAll" 
                   @onchange="ToggleSelectAll" 
                   checked="@IsAllSelected" />
            <label class="form-check-label fw-bold" for="selectAll">
                Select All (@GetBilldemand.Count demands)
            </label>
        </div>

        <!-- Inline Checkboxes -->
        <div class="demands-checkboxes">
                    @foreach (var demand in GetBilldemand)
            {
                <div class="form-check form-check-inline demand-item">
                    <input class="form-check-input" 
                           type="checkbox" 
                           id="demand-@demand.DemandIrn" 
                                   checked="@demand.IsSelected"
                                   @onchange="@(() => ToggleDemand(demand))" />
                    <label class="form-check-label" for="demand-@demand.DemandIrn">
                                <span class="demand-irn">@demand.DemandIrn</span>
                        @if (!string.IsNullOrEmpty(demand.PartyName))
                        {
                            <span class="party-name">(@demand.PartyName)</span>
                        }
                       
                    </label>
                </div>
            }
        </div>

        <!-- Selected Count -->
        <div class="mt-3 text-muted">
            Selected: @SelectedCount out of @GetBilldemand.Count
        </div>
    </div>
}



